{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "npx @11ty/eleventy --pathprefix=/TwelveMedievalGhostStories/",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/TwelveMedievalGhostStories/",
    "start": "npx @11ty/eleventy --serve",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"deploy": "gh-pages -d _site --nojekyll"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/NinaZumel/TwelveMedievalGhostStories.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-navigation": "0.3.5",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "eleventy-plugin-images-responsiver": "^2.2.0",
    "luxon": "3.3.0",
    "markdown-it": "11.0.1",
    "markdown-it-anchor": "8.6.7",
    "markdown-it-attrs": "3.0.3",
    "markdown-it-footnote": "4.0.0"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "@11ty/eleventy-img": "^3.1.8",
    "@11ty/eleventy-plugin-bundle": "^1.0.4",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0"
  }
}
